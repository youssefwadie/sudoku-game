                onMouseClicked="#buttonNinePressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="9"/>
        <Canvas fx:id="canvas" height="450.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#canvasMouseClicked"
                width="450.0"/>
        <Button layoutX="626.0" layoutY="364.0" mnemonicParsing="false" onAction="#solve" prefHeight="60.0"
                prefWidth="70.0" text="SOLVE"/>
        <Button layoutX="528.0" layoutY="364.0" mnemonicParsing="false" onAction="#reset" prefHeight="60.0"
                prefWidth="70.0" text="RESET"/>
    </children>
</Pane>
