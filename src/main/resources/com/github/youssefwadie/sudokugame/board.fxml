<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>

<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="480.0"
      prefWidth="720.0" styleClass="base_pane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.github.youssefwadie.sudokugame.SudokuController">
    <stylesheets>
        <URL value="@style.css"/>
    </stylesheets>

    <children>
        <Button layoutX="528.0" layoutY="156.0" mnemonicParsing="false"
                onMouseClicked="#buttonOnePressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="1"/>
        <Button layoutX="587.0" layoutY="156.0" mnemonicParsing="false"
                onMouseClicked="#buttonTwoPressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="2"/>
        <Button layoutX="646.0" layoutY="156.0" mnemonicParsing="false"
                onMouseClicked="#buttonThreePressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="3"/>
        <Button  layoutX="528.0" layoutY="215.0" mnemonicParsing="false"
                onMouseClicked="#buttonFourPressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="4"/>
        <Button layoutX="587.0" layoutY="215.0" mnemonicParsing="false"
                onMouseClicked="#buttonFivePressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="5"/>
        <Button layoutX="646.0" layoutY="215.0" mnemonicParsing="false"
                onMouseClicked="#buttonSixPressed" prefHeight="50.0" prefWidth="50.0" styleClass="button" text="6"/>
